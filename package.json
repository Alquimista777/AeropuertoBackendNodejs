{
  "name": "restaurantes",
  "description": "Backend para restaurantes y platos",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./out/www.js",
    "test": "npm run compiletests && mocha ./tests/out/tests/**/test*.js",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src",
    "compiletests": "node ./node_modules/typescript/bin/tsc -p ./tests",
    "docs": "apidoc -i src/ -o apidocs/ && apidoc-markdown -p apidocs/ -o README.md",
    "postinstall":"gulp build",
    "deploy":"npm test "

  },
  "dependencies": {
    "apidoc": "^0.17.6",
    "apidoc-markdown": "^0.2.0",
    "body-parser": "~1.13.2",

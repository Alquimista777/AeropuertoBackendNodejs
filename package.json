    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.33",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^2.0.4",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "^0.0.29",
    "@types/express": "^4.0.35",
    "@types/gulp": "^4.0.2",
    "@types/gulp-less": "^0.0.30",
    "@types/gulp-mocha": "^0.0.30",
    "@types/gulp-nodemon": "^0.0.30",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.2.14",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.10",
    "@types/power-assert": "^1.4.29",
    "@types/serve-favicon": "^2.2.28",
    "@types/socket.io": "^1.4.31",
    "@types/supertest": "^2.0.3",
    "browser-sync": "^2.18.8",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "gulp-mocha": "^4.1.0",
